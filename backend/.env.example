# MongoDB connection string (replace <...> with your actual values)
MONGO_URI=mongodb://<username>:<password>@<host>.mongo.cosmos.azure.com:10255/?ssl=true&retrywrites=false&replicaSet=globaldb

# MongoDB database and collection
MONGO_DB_NAME=sandboxDB
MONGO_COLLECTION_NAME=users

# JWT secret used to sign authentication tokens
JWT_SECRET=<your-secure-jwt-secret>